# Crossmint (client-side)
# Create a Crossmint client key with wallets/auth scopes.
NEXT_PUBLIC_CROSSMINT_API_KEY=ck_staging_replace_me
NEXT_PUBLIC_CROSSMINT_EVM_CHAIN=base-sepolia
# Optional: browser partner base URL override.
# Default is same-origin proxy: /api/partner/proxy
# NEXT_PUBLIC_MACHINES_PARTNER_BASE_URL=/api/partner/proxy
NEXT_PUBLIC_EVM_SOURCE_CHAIN_ID=84532
NEXT_PUBLIC_EVM_RUSD_TOKEN=0x10b5Be494C2962A7B318aFB63f0Ee30b959D000b
NEXT_PUBLIC_DEMO_AUTOFUND_TESTNET=true
NEXT_PUBLIC_DEMO_AUTOFUND_SERVER_FALLBACK=false

# Machines partner API (server-only)
MACHINES_PARTNER_BASE_URL=https://api-dev.machines.cash
MACHINES_PARTNER_API_KEY=partner_key_replace_me
MACHINES_PARTNER_DEFAULT_SCOPES=users.read,users.write,kyc.read,kyc.write,cards.read,cards.write,cards.secrets.read,deposits.read,deposits.write,balances.read,transactions.read,withdrawals.write

# Optional static external user prefix for mapping
MACHINES_PARTNER_EXTERNAL_USER_PREFIX=crossmint-demo

# Dev-only Base Sepolia auto-funding (create deposit -> mint 100 rUSD -> transfer to user)
DEV_RUSD_AUTOFUND_ENABLED=false
DEV_RUSD_CHAIN_ID=84532
DEV_RUSD_RPC_URL=https://sepolia.base.org
DEV_RUSD_TOKEN_ADDRESS=0x10b5Be494C2962A7B318aFB63f0Ee30b959D000b
DEV_RUSD_MINTER_PRIVATE_KEY=0xreplace_me
